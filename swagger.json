            "post":{
                "summary":"register a new admin",
                "tags":[
                    "Admin"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "type": "object",
                        "schema": {
                            "properties": {
                                "data": {
                                    "properties": {
                                        "email": {
                                            "type": "string",
                                            "example": "admin@gmail.com"
                                        },
                                        "password": {
                                            "type": "string",
                                            "example": "12345"
                                        },
                                        "name": {
                                            "type": "string",
                                            "example": "admin"
                                        }
                                    }
                                }
                            }
                        }
                    }
                ],
                "responses":{
                    "200":{
                        "description":"ok"
                    }
                }
            }
        },
